<head>
    <title>Movies</title>
    <link rel="stylesheet" href="styles.css" />
  </head>
<h1> Movie List </h1>
<table>
  <tr>
  <th>Title</th>
  <th>Year</th>
  <th>Rating</th>
  <th>Genre</th>
  <th>Studio</th>
</tr>
  <% @movies.each do |movie| %>
    <tr>
      <td>
        <a href="/movies/:id"><%= "#{movie["movie"]}" %></a>
      </td>
      <td>
        <%= "#{movie["year"]}" %>
      </td>
      <td>
        <%= "#{movie["rating"]}" %>
      </td>
      <td>
        <%= "#{movie["genre"]}" %>
      </td>
      <td>
        <%= "#{movie["studio"]}" %>
      </td>
    </tr>
  <% end %>
</table>

